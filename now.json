{
  "version":1,
  "scale": {
    "sfo1": {
      "min": 1,
      "max": "auto"
    }
  },
  "env": {
    "DEBUG": "shared:rethinkdb:db-query-cache",
    "S3_TOKEN": "@s3-token",
    "S3_SECRET": "@s3-secret",
    "COMPOSE_RETHINKDB_PASSWORD": "@rethinkdb-password",
    "COMPOSE_RETHINKDB_URL": "@rethinkdb-url",
    "COMPOSE_RETHINKDB_PORT": "@rethinkdb-port",
    "AWS_RETHINKDB_PASSWORD": "@rethinkdb-password",
    "AWS_RETHINKDB_URL": "@rethinkdb-url",
    "AWS_RETHINKDB_PORT": "@rethinkdb-port",
    "COMPOSE_REDIS_PASSWORD": "@compose-redis-password",
    "COMPOSE_REDIS_URL": "@compose-redis-url",
    "COMPOSE_REDIS_PORT": "@compose-redis-port",
    "REDIS_CACHE_PORT": "@compose-redis-port",
    "REDIS_CACHE_URL": "@compose-redis-url",
    "REDIS_CACHE_PASSWORD": "@compose-redis-password",
    "TWITTER_OAUTH_CLIENT_SECRET": "@twitter-oauth-client-secret",
    "FACEBOOK_OAUTH_CLIENT_ID": "@facebook-oauth-client-id",
    "FACEBOOK_OAUTH_CLIENT_SECRET": "@facebook-oauth-client-secret",
    "GOOGLE_OAUTH_CLIENT_SECRET": "@google-oauth-client-secret",
    "GITHUB_OAUTH_CLIENT_SECRET": "@github-oauth-client-secret",
    "SESSION_COOKIE_SECRET": "@session-cookie-secret",
    "API_TOKEN_SECRET": "@api-token-secret",
    "ALGOLIA_APP_ID": "@algolia-app-id",
    "ALGOLIA_API_SECRET": "@algolia-api-secret",
    "ENCRYPTION_KEY": "@encryption-key",
    "SENDGRID_API_KEY": "@sendgrid-api-key"
  }
}